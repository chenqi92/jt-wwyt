<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lyc.wwyt.dao.TDwszxxDao">

    <resultMap type="com.lyc.wwyt.entity.TDwszxxEntity" id="TDwszxxMap">
        <result property="id" column="id"/>
        <result property="tyshxydm" column="TYSHXYDM"/>
        <result property="dwmc" column="DWMC"/>
        <result property="szbm" column="SZBM"/>
        <result property="dwlx" column="DWLX"/>
        <result property="dwbh" column="DWBH"/>
        <result property="bz" column="BZ"/>
        <result property="createTime" column="create_time"/>
        <result property="createBy" column="create_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="deleteMark" column="delete_mark"/>
    </resultMap>

    <sql id="baseColumn">
        id
        ,
        TYSHXYDM,        
        DWMC,        
        SZBM,        
        DWLX,        
        DWBH,        
        BZ,        
        create_time,        
        create_by,        
        update_time,        
        update_by,        
        delete_mark
    </sql>

    <select id="selectList" resultType="com.lyc.wwyt.vo.TDwszxxVO">
        select
        id,
        TYSHXYDM,
        DWMC,
        SZBM,
        DWLX,
        DWBH,
        BZ,
        create_time,
        create_by,
        update_time,
        update_by,
        delete_mark
        from t_dwszxx
        where delete_mark = 0
        <if test="tDwszxxDTO != null and tDwszxxDTO != ''">
            AND id = #{tDwszxxDTO.id}
        </if>
    </select>

</mapper>
