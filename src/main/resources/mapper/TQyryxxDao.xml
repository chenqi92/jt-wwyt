        id,
        TYSHXYDM,
        xm,
        xb,
        rylx,
        mz,
        zgxl,
        zy,
        gzjl,
        zhbyyx,
        zjlx,
        zjhm,
        jrbdwrq,
        bz,
        create_time,
        create_by,
        update_time,
        update_by,
        delete_mark,
        company_code
        from t_qyryxx
        where delete_mark = 0
        <if test="tQyryxxDTO != null and tQyryxxDTO != ''">
            AND id = #{tQyryxxDTO.id}
        </if>
