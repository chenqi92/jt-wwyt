        <result property="gzjl" column="gzjl"/>
        <result property="zhbyyx" column="zhbyyx"/>
        <result property="zjlx" column="zjlx"/>
        <result property="zjhm" column="zjhm"/>
        <result property="jrbdwrq" column="jrbdwrq"/>
        <result property="bz" column="bz"/>
