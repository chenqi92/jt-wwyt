<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lyc.wwyt.dao.TJcpcsdxxDao">

    <resultMap type="com.lyc.wwyt.entity.TJcpcsdxxEntity" id="TJcpcsdxxMap">
        <result property="id" column="id"/>
        <result property="tyshxydm" column="TYSHXYDM"/>
        <result property="xm" column="XM"/>
        <result property="szbm" column="SZBM"/>
        <result property="gw" column="GW"/>
        <result property="jcxm" column="JCXM"/>
        <result property="jcpc" column="JCPC"/>
        <result property="jccs" column="JCCS"/>
        <result property="createTime" column="create_time"/>
        <result property="createBy" column="create_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="deleteMark" column="delete_mark"/>
    </resultMap>

    <sql id="baseColumn">
        id
        ,
        TYSHXYDM,        
        XM,        
        SZBM,        
        GW,        
        JCXM,        
        JCPC,        
        JCCS,        
        create_time,        
        create_by,        
        update_time,        
        update_by,        
        delete_mark
    </sql>

    <select id="selectList" resultType="com.lyc.wwyt.vo.TJcpcsdxxVO">
        select
        id,
        TYSHXYDM,
        XM,
        SZBM,
        GW,
        JCXM,
        JCPC,
        JCCS,
        create_time,
        create_by,
        update_time,
        update_by,
        delete_mark
        from t_jcpcsdxx
        where delete_mark = 0
        <if test="tJcpcsdxxDTO != null and tJcpcsdxxDTO != ''">
            AND id = #{tJcpcsdxxDTO.id}
        </if>
    </select>

</mapper>
